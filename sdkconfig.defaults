# HB-RF-ETH Default Configuration

# SNMP Support for Monitoring
CONFIG_LWIP_SNMP=y
CONFIG_LWIP_SNMP_MAX_OBJ_ID_LEN=50
CONFIG_LWIP_MIB2_CALLBACKS=y

# HTTP Server
CONFIG_HTTPD_MAX_URI_HANDLERS=15

# Ethernet - ESP32 RMII Clock Output on GPIO17 for LAN8720
CONFIG_ETH_ENABLED=y
CONFIG_ETH_USE_ESP32_EMAC=y
CONFIG_ETH_PHY_INTERFACE_RMII=y
CONFIG_ETH_RMII_CLK_OUTPUT=y
CONFIG_ETH_RMII_CLK_OUT_GPIO=17
CONFIG_ETH_DMA_BUFFER_SIZE=1600
CONFIG_ETH_DMA_RX_BUFFER_NUM=20
CONFIG_ETH_DMA_TX_BUFFER_NUM=20
